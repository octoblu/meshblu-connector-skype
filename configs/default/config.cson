title: "Default Configuration"
type: "object"
properties:
  options:
    title: "Options"
    type: "object"
    properties:
      url:
        title: "Meeting Url"
        type: "string"
      state:
        title: "State"
        type: "string"
        "enum": [
          'Join Meeting'
          'End Meeting'
        ]
        default: "End Meeting"
      enable_video:
        title: "Enable Video"
        type: "boolean"
        default: true
      mute_toggle:
        title: "Mute Self"
        type: "boolean"
        default: false
    required: [
      "url"
      "state"
      "enable_video"
      "mute_toggle"
    ]
