{
  "name": "meshblu-connector-skype",
  "version": "4.6.4",
  "description": "Meshblu Connector ",
  "homepage": "https://github.com/octoblu/meshblu-connector-skype",
  "bugs": "https://github.com/octoblu/meshblu-connector-skype/issues",
  "author": {
    "name": "Octoblu",
    "url": "https://github.com/octoblu"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/octoblu/meshblu-connector-skype.git"
  },
  "scripts": {
    "build": "./build.sh",
    "start": "meshblu-connector",
    "test": "istanbul cover _mocha -- -R spec --require coffee-coverage/register-istanbul && mocha --reporter json > coverage/mocha.json",
    "generate:package": "meshblu-connector-packager",
    "generate:schema": "meshblu-connector-schema-generator > schemas.json"
  },
  "bin": "command.js",
  "pkg": {
    "targets": [
      "node7-macos-x64",
      "node7-linux-x64",
      "node7-linux-x86",
      "node7-win-x64",
      "node7-win-x86"
    ],
    "scripts": [
      "package.json",
      "index.js",
      "src/*.js",
      "jobs/**/*.js",
      "configs/**/*.js"
    ],
    "assets": [
      "jobs/**/*.json",
      "configs/**/*.json",
      "bin/*.node"
    ]
  },
  "devDependencies": {
    "chai": "^4.0.1",
    "coffee-coverage": "^2.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "pkg": "^3.0.5",
    "segfault-handler": "^1.0.0",
    "sinon": "^2.3.2",
    "sinon-chai": "^2.10.0"
  },
  "dependencies": {
    "async": "^2.4.1",
    "coffee-script": "^1.12.6",
    "debug": "^2.6.8",
    "edge": "^6.5.1",
    "eventemitter2": "^3.0.2",
    "fs-cson": "^0.3.2",
    "lodash": "^4.16.6",
    "lodash.isempty": "^4.4.0",
    "meshblu-config": "^7.0.0",
    "meshblu-connector-cli": "^1.0.5",
    "meshblu-connector-runner": "^10.8.4",
    "meshblu-connector-schema-generator": "^2.1.0",
    "ps-node": "^0.1.6"
  },
  "meshbluConnector": {
    "githubSlug": "octoblu/meshblu-connector-skype"
  },
  "platformDependencies": {
    "darwin": [],
    "win32": [],
    "win64": [],
    "linux": [],
    "android": null,
    "ios": null
  },
  "license": "MIT",
  "keywords": [
    "meshblu-connector",
    "gateblu"
  ]
}
